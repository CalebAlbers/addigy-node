{
    "name": "addigy",
    "version": "2.2.1",
    "description": "",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "scripts": {
        "build": "run-s -l clean test tsc copy",
        "build:check": "tsc --noEmit",
        "clean": "rimraf coverage dist tmp",
        "copy": "cpy package.json dist && cpy README.md dist",
        "lint": "eslint \"src/**/*.ts\" --fix",
        "postinstall": "[ -d '.husky' ] && husky install || true",
        "qa": "run-s -l lint test clean build:check",
        "semantic-release": "semantic-release",
        "test": "jest",
        "tsc": "tsc -p tsconfig.build.json"
    },
    "keywords": [
        "addigy",
        "addigy api",
        "rest api",
        "rest"
    ],
    "author": "Caleb Albers <calebalbers@gmail.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/calebalbers/addigy-node"
    },
    "dependencies": {
        "form-data": "4.0.0",
        "got": "11.8.2",
        "uuid": "8.3.2"
    },
    "devDependencies": {
        "@pliancy/eslint-config-ts": "0.0.5",
        "@pliancy/semantic-release-config-npm": "^2.1.0",
        "@types/got": "9.6.11",
        "@types/jest": "26.0.23",
        "@types/node": "15.12.5",
        "@types/uuid": "8.3.1",
        "commitizen": "^4.2.4",
        "cpy-cli": "3.1.1",
        "cz-conventional-changelog": "3.3.0",
        "gh-pages": "3.2.3",
        "husky": "^7.0.1",
        "jest": "27.0.6",
        "npm-run-all": "^4.1.5",
        "open": "8.2.1",
        "rimraf": "3.0.2",
        "ts-jest": "27.0.3",
        "typedoc": "0.21.2",
        "typescript": "4.3.4"
    },
    "volta": {
        "node": "14.17.1",
        "yarn": "1.22.10"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
